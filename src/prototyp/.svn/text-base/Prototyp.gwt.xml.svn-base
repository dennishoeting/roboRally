  <inherits name='com.smartgwt.SmartGwtNoTheme'/>
  <inherits name='com.smartclient.theme.blackops.BlackOps'/>
  <inherits name='org.vaadin.gwtgraphics.GWTGraphics' /> 
  <inherits name='com.allen_sauer.gwt.voices.gwt-voices' />
