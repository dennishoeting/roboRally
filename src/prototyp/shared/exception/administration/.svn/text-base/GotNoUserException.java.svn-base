package prototyp.shared.exception.administration;

import prototyp.server.view.AdministrationImpl;

/**
 * GotNoUserException - Wird geworfen, wenn keine Nutzer aus der Datenbank
 * gelesen werden konnten oder wenn keine Nutzer vorhanden. sind.
 * 
 * @author Andreas
 * @version 1.0
 * 
 * @see AdministrationImpl
 */
public class GotNoUserException extends Exception {

	/** Seriennummer */
	private static final long serialVersionUID = 4788910575068765304L;

	/**
	 * Konstruktor
	 */
	public GotNoUserException() {
		super("Es konnten keine Nutzer geladen werden.");
	}
}
