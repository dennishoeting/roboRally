package prototyp.shared.exception.playingboard;

import prototyp.client.view.Page;

/**
 * Exception die geworfen wird, wenn die Checkpoints nicht erreichbar sind.
 * 
 * @author Marcus
 * @version 1.0
 */
public class CheckpointFieldNumeratedExecption extends PlayingboardException {

	/**
	 * Seriennummer
	 */
	private static final long serialVersionUID = 2993927398971158975L;

	/**
	 * Konstruktor
	 */
	public CheckpointFieldNumeratedExecption() {
	}

	/**
	 * Liefert die Fehlermeldung
	 * 
	 * @return Fehlermeldung
	 */
	@Override
	public String getMessage() {
		return Page.props.exception_playingboard_chechpointfieldNumeratedException();
	}

}
