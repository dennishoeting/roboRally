package prototyp.shared.exception.mail;

import prototyp.client.view.Page;
import prototyp.shared.exception.RoboException;

/**
 * EMailNotFoundException - Wird geworfen, wenn die E-Mail Adresse nicht
 * gefunden wird
 * 
 * @author Marina
 * @version 1.0
 */
public class MailNotFoundException extends RoboException {

	/**
	 * Seriennummer
	 */
	private static final long serialVersionUID = -8832498462630172046L;

	/**
	 * Konstruktor
	 */
	public MailNotFoundException() {

	}

	@Override
	public String getMessage() {
		return Page.props.exception_mail_mailNotFoundException();
	}
}