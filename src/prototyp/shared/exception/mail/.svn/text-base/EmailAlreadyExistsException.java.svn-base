package prototyp.shared.exception.mail;

import prototyp.client.view.Page;
import prototyp.server.view.AdministrationImpl;
import prototyp.shared.exception.RoboException;

/**
 * EmailAlreadyExistsException - Wird geworfen, wenn die E-Mailadresse schon
 * vorhanden ist.
 * 
 * @author Andreas, Jens
 * @version 1.1
 * 
 * @see AdministrationImpl
 */
public class EmailAlreadyExistsException extends RoboException {

	/** Seriennummer */
	private static final long serialVersionUID = 956513958145759349L;

	/**
	 * Konstruktor
	 */
	public EmailAlreadyExistsException() {

	}

	@Override
	public String getMessage() {
		return Page.props.exception_mail_eMailAlreadyExistsException();
	}
}
