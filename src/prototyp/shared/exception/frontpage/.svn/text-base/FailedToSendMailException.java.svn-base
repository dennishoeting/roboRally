package prototyp.shared.exception.frontpage;

import prototyp.client.view.Page;
import prototyp.server.view.FrontPageImpl;
import prototyp.shared.exception.RoboException;

/**
 * FailedToSendMailException - Wird geworfen, wenn die E-Mail nicht versandt
 * werden konnte.
 * 
 * @author Marina
 * @version 1.0
 * 
 * @see FrontPageImpl
 */
public class FailedToSendMailException extends RoboException {

	/**
	 * Seriennummer
	 */
	private static final long serialVersionUID = 1533737070736683832L;

	/**
	 * Konstruktor
	 */
	public FailedToSendMailException() {

	}

	@Override
	public String getMessage() {
		return Page.props.exception_frontpage_failedToSendMailException();
	}
}