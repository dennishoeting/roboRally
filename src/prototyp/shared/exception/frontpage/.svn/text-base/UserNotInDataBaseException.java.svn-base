package prototyp.shared.exception.frontpage;

import prototyp.client.view.Page;
import prototyp.shared.exception.RoboException;

/**
 * Wenn ein Username nicht existiert beim einloggen.
 * 
 * @author timo
 * 
 */
public class UserNotInDataBaseException extends RoboException {

	/**
	 * Seriennummer
	 */
	private static final long serialVersionUID = -153880633580201355L;

	/**
	 * Default
	 */
	public UserNotInDataBaseException() {

	}

	@Override
	public String getMessage() {
		return Page.props.exception_frontpage_userNotInDataBaseException();
	}

}
