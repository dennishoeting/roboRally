package prototyp.shared.exception.frontpage;

import prototyp.client.view.Page;
import prototyp.shared.exception.RoboException;

/**
 * UserAlreadyOnlineException - Wird geworfen wenn ein User der bereits online
 * ist, versucht sich nochmal einzuloggen
 * 
 * @author Kamil
 * @version 1.0
 */
public class UserAlreadyOnlineException extends RoboException {

	/**
	 * Seriennummer
	 */
	private static final long serialVersionUID = -826873181357792449L;

	/**
	 * Default
	 */
	public UserAlreadyOnlineException() {

	}

	@Override
	public String getMessage() {
		return Page.props.exception_userAlreadyOnlineException();
	}
}
