package prototyp.shared.field;

import prototyp.shared.util.WallInfo;

/**
 * abstrakte Oberklasse für alle Felder auf denen Sicherungskopien von Robotern
 * abgelegt werden können
 * 
 * @author Marcus
 * 
 */
public abstract class BackupField extends WallField {

	/**
	 * Seriennummer
	 */
	private static final long serialVersionUID = -1538760221070203819L;

	/**
	 * Default-Konstruktor für Serializable-Interface
	 */
	public BackupField() {
	}

	/**
	 * Konstruktor
	 * 
	 * @param wallInfo
	 */
	public BackupField(WallInfo wallInfo) {
		super(wallInfo);
	}
}
